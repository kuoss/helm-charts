              "repository": {
                "type": "string",
                "description": "Image repository for test framework."
              },
              "pullPolicy": {
                "type": "string",
                "enum": [
                  "Always",
                  "IfNotPresent",
                  "Never"
                ],
                "description": "Image pull policy."
              },
              "tag": {
                "type": "string",
                "description": "Tag of the test framework image."
              },
              "digest": {
                "type": [
                  "string",
                  "null"
                ],
                "description": "Digest of the image."
              }
            },
            "required": [
              "repository",
              "tag"
            ]
          }
        },
        "required": [
          "enabled",
          "image"
        ]
      },
      "imagePullSecrets": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "description": "A list of secret names for pulling images."
      },
      "serviceAccount": {
        "type": "object",
        "properties": {
          "create": {
            "type": "boolean",
            "description": "Whether to create a new service account."
          },
          "annotations": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            },
            "description": "Annotations for the service account."
          },
          "name": {
            "type": [
              "string",
              "null"
            ],
            "description": "Name of the service account. If not set, a name will be generated."
          }
        },
        "required": [
          "create"
        ]
      },
      "rbac": {
        "type": "object",
